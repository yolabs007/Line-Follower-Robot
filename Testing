#include <AFMotor.h>


AF_DCMotor motor1(1, MOTOR12_1KHZ); 
AF_DCMotor motor2(4, MOTOR12_1KHZ);  

 

void setup() 
{       
  motor1.setSpeed(255);
  motor2.setSpeed(255);

}

void loop(){

  motor1.run(FORWARD);  
  motor2.run(FORWARD); 
  delay(5000);

  motor1.run(BACKWARD);  
  motor2.run(BACKWARD); 
  delay(3000);

}
